{
	"compilerOptions": {
		"target": "ES2022",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"sourceMap": true,
		"outDir": "dist",
		"strict": true,
		"lib": ["esnext", "dom"],
		"esModuleInterop": true,

		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true
	}
}
